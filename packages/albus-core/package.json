{
  "name": "@mfactory-lab/albus-core",
  "version": "0.0.6",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/mfactory-lab/albus.git",
    "directory": "packages/albus-core"
  },
  "sideEffects": false,
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lib:dev": "vite build --watch",
    "build": "vite build",
    "release": "release-it",
    "release:ci": "release-it --ci",
    "test": "vitest"
  },
  "dependencies": {
    "@metaplex-foundation/mpl-token-metadata": "^2.13.0",
    "@stablelib/random": "^1.0.2",
    "@stablelib/sha256": "^1.0.1",
    "@stablelib/x25519": "^1.0.3",
    "@stablelib/ed25519": "^1.0.3",
    "@stablelib/xchacha20poly1305": "^1.0.1",
    "@solana/web3.js": "^1.77.3",
    "axios": "^1.4.0",
    "crypto-browserify": "^3.12.0",
    "ffjavascript": "^0.2.59",
    "did-jwt-vc": "^3.2.2",
    "did-jwt": "^7.2.2",
    "did-resolver": "^4.1.0",
    "key-did-resolver": "^3.0.0",
    "web-did-resolver": "^2.0.24",
    "circomlib": "^2.0.5",
    "circomlibjs": "^0.1.7",
    "snarkjs": "^0.7.0",
    "poseidon-encryption": "^1.0.2",
    "uint8arrays": "^4.0.4"
  },
  "devDependencies": {
    "circom_tester": "^0.0.19"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "release-it": {
    "hooks": {
      "after:bump": "pnpm build"
    },
    "github": {
      "release": false
    },
    "git": false,
    "_plugins": {
      "@release-it/bumper": {
        "out": {
          "file": "../albus-sdk/package.json",
          "path": "dependencies.@mfactory-lab/albus-core"
        }
      }
    }
  }
}
