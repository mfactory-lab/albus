    "@stablelib/xchacha20poly1305": "^1.0.1",
    "axios": "^1.4.0",
    "did-resolver": "^3.2.2",
    "key-did-resolver": "^3.0.0",
    "snarkjs": "*",
    "uint8arrays": "^3.1.1",
    "web-did-resolver": "^2.0.27"
