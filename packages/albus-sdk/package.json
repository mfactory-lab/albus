{
  "name": "@albus/sdk",
  "version": "0.0.6",
  "description": "Albus Protocol SDK",
  "sideEffects": false,
  "type": "module",
  "main": "./src/index",
  "_prod": {
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs"
      }
    },
    "files": [
      "dist"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mfactory-lab/albus.git",
    "directory": "packages/albus-sdk"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "unbuild",
    "generate": "DEBUG='(solita|rustbin):(info|error)' solita",
    "_prepare": "pnpm run build"
  },
  "dependencies": {
    "@albus/core": "workspace:*",
    "@coral-xyz/anchor": "^0.28.0",
    "@metaplex-foundation/beet": "^0.7.1",
    "@metaplex-foundation/beet-solana": "^0.4.0",
    "@solana/web3.js": "^1.77.3"
  },
  "devDependencies": {
    "@metaplex-foundation/solita": "^0.19.4"
  },
  "peerDependencies": {
    "snarkjs": "*"
  }
}
