{
  "name": "@mfactory-lab/albus-sdk",
  "version": "0.2.4",
  "description": "Albus Protocol SDK",
  "_type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/mfactory-lab/albus.git",
    "directory": "packages/albus-sdk"
  },
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "generate": "DEBUG='(solita|rustbin):(info|error)' solita",
    "release": "release-it --ci",
    "test": "vitest"
  },
  "dependencies": {
    "@mfactory-lab/albus-core": "workspace:^",
    "@coral-xyz/anchor": "^0.29.0",
    "@metaplex-foundation/beet": "^0.7.1",
    "@metaplex-foundation/beet-solana": "^0.4.0",
    "@metaplex-foundation/mpl-token-metadata": "^2.13.0",
    "@solana/web3.js": "^1.77.3",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@mfactory-lab/release-it-config": "workspace:^",
    "@metaplex-foundation/solita": "^0.20.0"
  },
  "peerDependencies": {
    "snarkjs": "*"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  }
}
