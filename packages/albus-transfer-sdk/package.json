{
  "name": "@mfactory-lab/albus-transfer-sdk",
  "version": "0.0.11",
  "description": "Albus Transfer SDK",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/mfactory-lab/albus.git",
    "directory": "packages/albus-transfer-sdk"
  },
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "release": "release-it --ci",
    "generate": "DEBUG='(solita|rustbin):(info|error)' solita && eslint --fix --ext .ts"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@metaplex-foundation/beet": "^0.7.1",
    "@solana/spl-token": "^0.3.8",
    "@solana/web3.js": "^1.77.3"
  },
  "devDependencies": {
    "@mfactory-lab/release-it-config": "workspace:^",
    "@metaplex-foundation/solita": "^0.20.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  }
}
