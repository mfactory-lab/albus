{
  "name": "albus",
  "packageManager": "pnpm@7.13.0",
  "scripts": {
    "api:gen": "pnpm --filter=sdk run api:gen && eslint --fix --ext .ts packages/albus-sdk/src/**",
    "ui:dev": "pnpm --filter=ui run dev",
    "cli": "pnpm --filter=cli run cli",
    "lint:fix": "eslint --cache --fix --ext .js,.vue,.ts ./packages/**",
    "amman:start": "CI=1 DEBUG='amman:(error|info|debug)*' amman start",
    "amman:stop": "amman stop",
    "test": "CI=1 ts-node-esm tests/albus.ts"
  },
  "dependencies": {
    "@albus/sdk": "workspace:*",
    "@verified-transfer/sdk": "workspace:verified-transfer-sdk",
    "@project-serum/anchor": "^0.26.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.5",
    "@metaplex-foundation/js": "^0.18.3",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.56",
    "@swc/helpers": "^0.5.1",
    "@swc/jest": "^0.2.26",
    "@types/bn.js": "^5.1.1",
    "@types/bs58": "^4.0.1",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.16.3",
    "chai": "^4.3.7",
    "eslint": "^8.39.0",
    "mocha": "^10.2.0",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.9.5"
  }
}
