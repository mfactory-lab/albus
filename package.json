{
  "name": "albus",
  "packageManager": "pnpm@7.13.0",
  "scripts": {
    "api:gen": "pnpm --filter=sdk run api:gen && eslint --fix --ext .ts ./packages/sdk/src/**",
    "ui:dev": "pnpm --filter=ui run dev",
    "cli": "pnpm --filter=cli run cli",
    "lint:fix": "eslint --cache --fix --ext .js,.vue,.ts ./packages/**",
    "amman:start": "CI=1 DEBUG='amman:(error|info|debug)*' amman start",
    "amman:stop": "amman stop",
    "test": "CI=1 ts-node-esm tests/albus.ts"
  },
  "dependencies": {
    "@project-serum/anchor": "^0.26.0",
    "@albus/sdk": "workspace:*"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38",
    "@metaplex-foundation/js": "^0.18.3",
    "@swc/core": "^1.3",
    "@types/node": "^18.15",
    "@types/bn.js": "^5.1.0",
    "@types/bs58": "^4.0.1",
    "@types/mocha": "^10.0.0",
    "@types/chai": "^4.3.4",
    "eslint": "^8.34.0",
    "mocha": "^10.2.0",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.9.5",
    "chai": "^4.3.7"
  }
}
