{
  "name": "@albus/monorepo",
  "packageManager": "pnpm@8.6.0",
  "scripts": {
    "api:gen": "pnpm --filter=sdk run api:gen && eslint --fix --ext .ts packages/albus-sdk/src/**",
    "cli": "pnpm --filter=@albus/cli run cli",
    "lint:fix": "eslint --cache --fix --ext .js,.vue,.ts ./packages/**",
    "test": "pnpm --filter=@albus/tests run test"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "eslint": "^8.42.0",
    "typescript": "^4.9.5",
    "@types/node": "^18.16.3"
  }
}
